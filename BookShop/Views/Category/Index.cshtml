@model List<Category>
<h1>Here is my Category List</h1>
<table class="table table-bordered">
	<thead>
	<tr>
		<td>Name</td>
		<td>Description</td>
	</tr>
	</thead>
	<tbody>
		@foreach (var category in Model)
		{
		<tr>
			<td>
				@category.Name
			</td>
			<td>
				@category.Description
			</td>
		</tr>
		}
	</tbody>
	
</table>